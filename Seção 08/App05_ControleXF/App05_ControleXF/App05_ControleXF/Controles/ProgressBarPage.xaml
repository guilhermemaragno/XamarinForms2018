<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App05_ControleXF.Controles.ProgressBarPage">
    <ContentPage.Content>
        <StackLayout>

            <ProgressBar x:Name="Bar01" Progress="0.25" />

            <ProgressBar x:Name="Bar02" Progress="0.5" />

            <ProgressBar x:Name="Bar03" Progress="0.85" />

            <Button Text="Alterar Valores" Clicked="AlterarValores" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>